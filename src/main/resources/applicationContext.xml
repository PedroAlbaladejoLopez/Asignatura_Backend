<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:context="http://www.springframework.org/schema/context"
    xsi:schemaLocation="
    http://www.springframework.org/schema/beans   
    http://www.springframework.org/schema/beans/spring-beans-3.1.xsd
    http://www.springframework.org/schema/context 
    http://www.springframework.org/schema/context/spring-context-3.1.xsd
">
	<!-- Creación de un Bean con atributos por constructor y por setter-->
	<bean id="recurso" class="es.mde.et.madoc.panacea_backend.models.Recurso">
		<constructor-arg name="M1N" value="FUTER"></constructor-arg>
		<property name="uCO" value="BRIPAC"></property>
	</bean>
	
	<bean id="recurso2" class="es.mde.et.madoc.panacea_backend.models.Recurso">
		<property name="uCO" value="BRIMZ"></property>
	</bean>
	
	<!-- Creación de un Bean que contiene otros Bean-->
	<bean id="activacion" class="es.mde.et.madoc.panacea_backend.models.Activacion">
		<property name="recursos">
			<set>
                <ref bean="recurso"/>
				<ref bean="recurso2"/>
           </set>
		</property>
		
	</bean>
</beans>